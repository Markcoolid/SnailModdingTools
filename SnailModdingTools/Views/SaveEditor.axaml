<UserControl xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:SnailModdingTools.ViewModels"
		xmlns:local="clr-namespace:SnailModdingTools;assembly=SnailModdingTools"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="SnailModdingTools.SaveEditor"
		xmlns:snaillib="clr-namespace:SnailModdingTools.lib;assembly=SnailModdingTools.lib">

	<Grid>
		<StackPanel>
			<TextBlock Text="Version"/>
			<TextBox Text="{Binding Version}"/>
		</StackPanel>
	</Grid>
	
</UserControl>
